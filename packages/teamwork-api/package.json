{
  "name": "teamwork-api",
  "scripts": {
    "build-ts": "tsc",
    "build": "npm run build-ts",
    "deploy:dev": "npm run build && gcloud --project teamwork-dev-74882 app deploy",
    "deploy:prod": "npm run build && gcloud --project teamwork-prod app deploy",
    "dev": "npm-run-all build watch-ts watch-node",
    "start": "npm run serve",
    "serve": "node lib/index.js",
    "watch-node": "nodemon lib/index.js",
    "watch-ts": "tsc -w"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-bearer-token": "^2.4.0",
    "firebase": "^5.10.0",
    "firebase-admin": "^8.3.0",
    "firebase-functions": "^3.2.0",
    "generate-password": "^1.4.2",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/morgan": "^1.7.36",
    "@types/node": "^12.7.1",
    "firebase-tools": "^7.2.2",
    "node-env-run": "^3.0.2",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "tslint-react-hooks": "^2.2.1",
    "typescript": "^3.5.3"
  },
  "private": true,
  "version": "0.0.1"
}
